
IMPORT github.com/earthly/earthly-command-example:main
IMPORT github.com/earthly/hello-world:main AS hello-dolly

all:
    BUILD +test-command
    BUILD +test-target

test-command:
    FROM alpine:3.13
    DO earthly-command-example+TOUCH --file=foo
    RUN test -f ./foo

test-target:
    FROM hello-dolly+hello
