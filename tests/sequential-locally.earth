    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
    RUN echo "mid $char"
