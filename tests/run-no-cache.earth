VERSION 0.7
test:
    FROM alpine:3.18
    
    RUN date | md5sum | head -c 32 > motd1
    SAVE ARTIFACT motd1 AS LOCAL motd1

    RUN --no-cache echo "Hello!"

    RUN date | md5sum | head -c 32 > motd2
    SAVE ARTIFACT motd2 AS LOCAL motd2

test-from:
    FROM alpine:3.18
    COPY +test/motd1 .
    COPY +test/motd2 .
